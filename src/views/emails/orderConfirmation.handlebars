<body style="background-color:#ffffff;">
  <div
    style="display:none;font-size:1px;color:#ffffff;line-height:1px;max-height:0px;max-width:0px;opacity:0;overflow:hidden;">
    Preview - Welcome to Your Company </div>
  <div style="margin:0px auto;max-width:520px;">
    <table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" style="width:100%;">
      <tbody>
        <tr>
          <td style="direction:ltr;font-size:0px;padding:20px 0;text-align:center;">

            <div class="mj-column-per-100 mj-outlook-group-fix"
              style="font-size:0px;text-align:left;direction:ltr;display:inline-block;vertical-align:top;width:100%;">
              <table border="0" cellpadding="0" cellspacing="0" role="presentation" style="vertical-align:top;"
                width="100%">
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:18px;font-weight:400;line-height:24px;text-align:left;color:#000000;">
                      <h1 style="margin: 0; font-size: 32px; line-height: 40px;">Hi {{this.user.name}}! <br>Thank you
                        for your order with RADA Inc.!</h1>
                    </div>
                  </td>
                </tr>
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:18px;font-weight:400;line-height:24px;text-align:left;color:#000000;">
                      Your Order will be Shipping to:</div>
                  </td>
                </tr>
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:18px;font-weight:400;line-height:24px;text-align:left;color:#000000;">
                      {{this.shipAddress.address}}
                      </div>
                       <div
                      style="font-family:Alata;font-size:18px;font-weight:400;line-height:24px;text-align:left;color:#000000;">
                      {{this.shipAddress.city}}, {{this.shipAddress.state}} {{this.shipAddress.postalcode}}</div>
                  </td>
                </tr>
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:18px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                      Items in your Order {{this.orderTotals.subTotal.totalQuantity}}</div>
                  </td>
                </tr>
                {{#each ordercart}}
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:14px;font-weight:400;line-height:24px;text-align:left;color:#333333;">
                      <table border="0" cellpadding="0" cellspacing="0" role="presentation"
                        style="border-collapse:collapse;border-spacing:0px;" class="mj-full-width-mobile">
                        <tbody>
                          <tr>
                            <td style="width:150px;" class="mj-full-width-mobile">
                              <img alt="image description" height="auto" src={{this.product_image}}
                                style="border:0;display:block;outline:none;text-decoration:none;height:auto;width:100%;font-size:14px;"
                                width="250">
                            </td>
                            <td style="padding:10px;">
                              <div class="pr-6">
                                <h3 class="text-sm">
                                  <a href="#"
                                    class="font-medium text-gray-700 hover:text-gray-800">{{this.product_name}}</a>
                                </h3>
                                <p class="mt-1 text-sm text-gray-500">Qty: {{this.quantity}}</p>
                                <p class="mt-1 text-sm text-gray-500">Color: {{this.product_color}}</p>
                              </div>
                              <p class="text-right text-sm font-medium text-gray-900">Price: ${{this.product_cost}}</p>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </td>
                </tr>
                {{/each}}
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:18px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                      Order Summary:</div>
                  </td>
                </tr>
                 <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                      SubTotal:</div>
                  </td>
                  <td>
                     <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                       ${{this.orderTotals.subTotal.totalPrice}}</div>
                   
                  </td>
                </tr>
                   <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                      Shipping:</div>
                  </td>
                  <td>
                     <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                        ${{this.orderTotals.shippingCost}}</div>
                  
                  </td>
                </tr>
                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                      Tax:</div>
                  </td>
                  <td>
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                       ${{this.orderTotals.tax}}</div>
                  
                  </td>
                </tr>
                     <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                      Order Total:</div>
                  </td>
                  <td>
                     <div
                      style="font-family:Helvetica, Arial, sans-serif;font-size:14px;font-weight:bold;line-height:24px;text-align:left;color:#434245;">
                       ${{this.orderTotals.grandTotal}}</div>
                 
                  </td>
                </tr>
             

                <tr>
                  <td align="left" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:14px;font-weight:400;line-height:24px;text-align:left;color:#333333;">
                      Have questions or need help? Email us at <a href="#"
                        style="color: #ea4a40; text-decoration: none;"> support@radainc.com </a></div>
                  </td>
                </tr>
                <tr>
                  <td align="center" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:14px;font-weight:400;line-height:24px;text-align:center;color:#333333;">
                      Barranquitas, PR 00794<br> Â© 2024 RADA Inc.</div>
                  </td>
                </tr>
                <tr>
                  <td align="center" style="font-size:0px;padding:10px 25px;word-break:break-word;">
                    <div
                      style="font-family:Alata;font-size:14px;font-weight:400;line-height:24px;text-align:center;color:#333333;">
                      Update your <a href="https://google.com" style="color: #ea4a40; text-decoration: none;">email
                        preferences</a> to choose the types of emails you receive, or you can <a
                        href="https://google.com" style="color: #ea4a40; text-decoration: none;"> unsubscribe </a>from
                      all future emails.</div>
                  </td>
                </tr>
              </table>
            </div>
            <!--[if mso | IE]>
            </td>
          
        </tr>
      
                  </table>
                <![endif]-->
          </td>
        </tr>
      </tbody>
    </table>
  </div>
  </div>
</body>

</html>